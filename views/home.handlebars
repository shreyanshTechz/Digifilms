    } @media (max-width: 450px) { .box { height: 200px; } } @media (max-width:
    950px) and (min-width: 450px) { .box { text-align: center; height: 180px; }
    } .cyan { border-top: 3px solid var(--cyan); } .red { border-top: 3px solid
    var(--red); } .blue { border-top: 3px solid var(--blue); } .orange {
    border-top: 3px solid var(--orange); } h2 { font-weight: var(--weight3); }
